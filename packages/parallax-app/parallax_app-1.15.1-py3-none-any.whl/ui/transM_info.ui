  <property name="styleSheet">
   <string notr="true">/* 1. Global Background &amp; Text */
QWidget {
    background-color: #121212;
    color: #E0E0E0;
    font-family: &quot;Consolas&quot;, &quot;Courier New&quot;;
    font-size: 8pt;
}

/* 2. Remove the grey boxes from all labels */
QLabel {
    background-color: transparent;
}


/* 3. Style the Values (The boxes with numbers) */
/* This removes the dark grey/sunken look of the number boxes */
QLineEdit, QDoubleSpinBox, QLabel#value_display {
    background-color: #1a1a1a; /* Very dark grey, almost black */
    border: 1px solid #333333; /* subtle border */
    border-radius: 2px;
    padding: 2px;
}

/* 4. TransM Section: Gold/Yellow */
#transM_, #transM_R_, #transM_T_label, #R_label, #T_label, #transM_title_label, #transM_q {
    color: #FFB800 !important;
}

/* 5. Arc Angle Section: Soft Red (Matches Pause Icon) */
#arc_angle_label, #label_6, #label_7, #label_8, #rx_label, #ry_label, #rz_label, #rz_push_btn {
    color: #F64F4F !important;
}

/* 6. Distance Section: Blue (Matches Save Icon) */
#l2_dist_label, #dist_travel_label, #l2_val, #dist_val, #l2_label, #travel_label {
    color: #2196F3 !important;
}

/* 7. Highlight the Headers specifically */
QLabel[text*=&quot;TransM&quot;], QLabel[text*=&quot;Arc Angle&quot;] {
    font-weight: bold;
    letter-spacing: 0.5px;
}

/* Greyout state when setEnabled(False) is called */
QPushButton#rz_push_btn:disabled {
    background-color: #1a1a1a; /* Darker than the background */
    color: #555555;            /* Dimmed grey text */
    border: 1px solid #222;    /* Darkened border */
}</string>
