{
  "name": "openbase-react-ui",
  "version": "1.2.6",
  "description": "Shared components between Openbase CLI React app and Openbase Cloud React app",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "barrels": "barrelsby src --location all --delete --exclude index.ts",
    "build": "npm run barrels && tsc && tsc-alias",
    "dev": "concurrently \"barrelsby src --location all --delete --exclude index.ts --watch\" \"tsc --watch --preserveWatchOutput --skipLibCheck --noEmitOnError false\" \"tsc-alias --watch\""
  },
  "peerDependencies": {
