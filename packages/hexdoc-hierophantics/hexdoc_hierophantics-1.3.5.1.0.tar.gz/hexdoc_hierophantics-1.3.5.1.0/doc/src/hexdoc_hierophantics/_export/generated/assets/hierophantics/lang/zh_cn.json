mmary": "\u521b\u5efa\u4e00\u4e2a\u5728\u6211\u4f20\u9001\u65f6\u6fc0\u6d3b\u7684\u89e6\u53d1\u6761\u4ef6\u3002\u56e0\u4e3a\u6211\u65e0\u6cd5\u6307\u660e\u7684\u539f\u56e0\uff0c\u6b64\u7c7b\u89e6\u53d1\u6761\u4ef6\u68c0\u6d4b\u4e0d\u5230\u8ddd\u79bb\u5728 1.5 \u683c\u4ee5\u4e0b\u7684\u4f20\u9001\u3002", "hierophantics.page.invert_trigger.summary": "\u53cd\u8f6c\u6570\u503c\u89e6\u53d1\u6761\u4ef6\u7684\u9608\u503c\u3002\u4f8b\u5982\uff0c\u201c\u901f\u5ea6\u9ad8\u4e8e 5.0\u201d\u4f1a\u53d8\u6210\u201c\u901f\u5ea6\u4f4e\u4e8e 5.0\u201d\u3002\u5bf9\u5176\u4ed6\u79cd\u7c7b\u7684\u89e6\u53d1\u6761\u4ef6\u6ca1\u6709\u6548\u679c\u3002", "text.autoconfig.hierophantics.title": "\u7075\u663e\u5b66\uff08Hierophantics\uff09\u914d\u7f6e", "text.autoconfig.hierophantics.category.client": "\u5ba2\u6237\u7aef\u8bbe\u7f6e", "text.autoconfig.hierophantics.option.client.itemHallucinations": "\u7269\u54c1\u5e7b\u89c6\u8bbe\u7f6e", "text.autoconfig.hierophantics.option.client.itemHallucinations.@Tooltip": "\u5185\u5d4c\u610f\u8bc6\u548c\u7e41\u5f62\u610f\u8bc6\u5076\u5c14\u4f1a\u5bfc\u81f4\u7269\u54c1\u5448\u73b0\u4e3a\u5176\u4ed6\u4e8b\u7269", "text.autoconfig.hierophantics.option.client.itemHallucinations.baseEmeraldRate": "\u57fa\u7840\u7eff\u5b9d\u77f3\u5e7b\u89c9\u6982\u7387", "text.autoconfig.hierophantics.option.client.itemHallucinations.baseEmeraldRate.@Tooltip": "\u6bcf\u4e2a\u5185\u5d4c\u610f\u8bc6\u8ffd\u52a0\u7684\u7eff\u5b9d\u77f3\u5e7b\u89c6\u6982\u7387", "text.autoconfig.hierophantics.option.client.itemHallucinations.maxEmeraldRate": "\u6700\u5927\u7eff\u5b9d\u77f3\u5e7b\u89c9\u6982\u7387", "text.autoconfig.hierophantics.option.client.itemHallucinations.maxEmeraldRate.@Tooltip": "\u7eff\u5b9d\u77f3\u5e7b\u89c6\u7684\u6700\u5927\u6982\u7387", "text.autoconfig.hierophantics.option.client.itemHallucinations.mediaRate": "\u5a92\u8d28\u5e7b\u89c9\u6982\u7387", "text.autoconfig.hierophantics.option.client.itemHallucinations.mediaRate.@Tooltip": "\u62e5\u6709\u7e41\u5f62\u610f\u8bc6\u6548\u679c\u65f6\u5a92\u8d28\u7269\u54c1\u5e7b\u89c6\u6982\u7387", "text.autoconfig.hierophantics.option.client.audioHallucinations": "\u58f0\u97f3\u5e7b\u542c\u8bbe\u7f6e", "text.autoconfig.hierophantics.option.client.audioHallucinations.@Tooltip": "\u5185\u5d4c\u610f\u8bc6\u548c\u7e41\u5f62\u610f\u8bc6\u5076\u5c14\u4f1a\u64ad\u653e\u53ea\u6709\u4f60\u80fd\u542c\u89c1\u7684\u58f0\u97f3", "text.autoconfig.hierophantics.option.client.audioHallucinations.baseVillagerRate": "\u57fa\u7840\u6751\u6c11\u5e7b\u89c9\u6982\u7387", "text.autoconfig.hierophantics.option.client.audioHallucinations.baseVillagerRate.@Tooltip": "\u6bcf\u4e2a\u5185\u5d4c\u610f\u8bc6\u8ffd\u52a0\u7684\u6751\u6c11\u58f0\u97f3\u64ad\u653e\u6982\u7387", "text.autoconfig.hierophantics.option.client.audioHallucinations.maxVillagerRate": "\u6700\u5927\u6751\u6c11\u5e7b\u89c9\u6982\u7387", "text.autoconfig.hierophantics.option.client.audioHallucinations.maxVillagerRate.@Tooltip": "\u64ad\u653e\u6751\u6c11\u58f0\u97f3\u7684\u6700\u5927\u6982\u7387", "text.autoconfig.hierophantics.option.client.audioHallucinations.allayRate": "\u60a6\u7075/\u7d2b\u6c34\u6676\u5e7b\u89c9\u6982\u7387", "text.autoconfig.hierophantics.option.client.audioHallucinations.allayRate.@Tooltip": "\u62e5\u6709\u7e41\u5f62\u610f\u8bc6\u6548\u679c\u65f6\u6bcf\u523b\u64ad\u653e\u60a6\u7075\u58f0\u97f3\u6216\u7d2b\u6c34\u6676\u53ee\u94c3\u58f0\u7684\u6982\u7387", "text.autoconfig.hierophantics.option.client.audioHallucinations.cooldown": "\u5e7b\u89c9\u51b7\u5374", "text.autoconfig.hierophantics.option.client.audioHallucinations.cooldown.@Tooltip": "\u4e24\u6b21\u5e7b\u542c\u95f4\u6700\u77ed\u7684\u95f4\u9694\u523b\u6570", "text.autoconfig.hierophantics.category.server": "\u670d\u52a1\u7aef\u8bbe\u7f6e", "text.autoconfig.hierophantics.option.server.maxMinds": "\u73a9\u5bb6\u6700\u5927\u5185\u5d4c\u610f\u8bc6\u6570", "text.autoconfig.hierophantics.option.server.mediaDiscount": "\u7e41\u5f62\u610f\u8bc6\u7684\u65bd\u6cd5\u6d88\u8017\u4e