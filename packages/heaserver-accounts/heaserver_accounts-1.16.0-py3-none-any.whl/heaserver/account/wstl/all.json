{
    "wstl": {
        "title": "HEA Server AWS Accounts Microservice",
        "actions": [
            {
                "name": "heaserver-awsaccounts-awsaccount-open",
                "type": "safe",
                "target": "item",
                "prompt": "Open"
            },
            {
                "name": "heaserver-accounts-awsaccount-get-self",
                "description": "View this account",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "View account"
            },
            {
                "name": "heaserver-accounts-account-get-actual",
                "description": "Get the actual object",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "Get actual"
            },
            {
                "name": "heaserver-accounts-awsaccount-get-searchitem",
                "description": "Search the awsaccount items",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "View searchitem"
            },
            {
                "name": "heaserver-accounts-awsaccount-searchitem-get-actual",
                "description": "Get the actual object",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "Get actual"
            },
            {
                "name": "heaserver-accounts-bucketitem-get-actual",
                "description": "Get the actual object",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "Get actual"
            },
            {
                "name": "heaserver-accounts-bucketitem-get-volume",
                "description": "View this item's volume",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "View volume"
            },
            {
                "name": "heaserver-accounts-awsaccount-get-trash",
                "description": "Open this volume's trash",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "Trash"
            },
            {
                "name": "heaserver-accounts-awsaccount-get-searchitem-self",
                "description": "Search the awsaccount items",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "View searchitem"
            },
            {
                "name": "heaserver-accounts-bucket-get-self",
                "description": "View this bucket",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "View account"
            },
            {
                "name": "heaserver-accounts-awsaccount-get-volume",
                "description": "View this account's volume",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "View volume"
            },
            {
                "name": "heaserver-accounts-awsaccount-get-properties",
                "description": "View and edit this AWS account's properties",
                "type": "unsafe",
                "target": "item cj-template",
                "action": "update",
                "prompt": "Properties",
                "inputs": [
                    {
                        "name": "id",
                        "prompt": "Id",
                        "readOnly": true,
                        "hea": {
                            "display": false
                        }
                    },
                    {
                        "name": "instance_id",
                        "prompt": "Instance id",
                        "readOnly": true,
                        "hea": {
                            "display": false
                        }
                    },
                    {
                        "name": "display_name",
                        "prompt": "Name",
                        "required": true,
                        "readOnly": true
                    },
                    {
                        "name": "type_display_name",
                        "prompt": "Type",
                        "readOnly": true,
                        "required": true
                    },
                    {
                        "name": "type",
                        "readOnly": true,
                        "required": true,
                        "hea": {
                            "display": false
                        }
                    },
                    {
                        "name": "full_name",
                        "prompt": "Full Name",
                        "readOnly": true
                    },
                    {
                        "name": "email_address",
                        "prompt": "Email Address",
                        "readOnly": true,
                        "hea": {
                            "type": "email"
                        }
                    },
                    {
                        "name": "phone_number",
                        "prompt": "Phone Number",
                        "readOnly": true,
                        "hea": {
                            "type": "tel"
                        }
                    },
                    {
                        "name": "alternate_contact_name",
                        "prompt": "Alternate Contact Name",
                        "readOnly": true
                    },
                    {
                        "name": "alternate_email_address",
                        "prompt": "Alternate Email Address",
                        "readOnly": true,
                        "hea": {
                            "type": "email"
                        }
                    },
                    {
                        "name": "alternate_phone_number",
                        "prompt": "Alternate Phone Number",
                        "readOnly": true,
                        "hea": {
                            "type": "tel"
                        }
                    },
                    {
                        "hea": {
                            "section": "collaborators",
                            "sectionPrompt": "Collaborators",
                            "optionsFromUrl": {
                                "path": "volumes/{volume_id}/bucketitems/",
                                "value": "id",
                                "text": "display_name"
                            }
                        },
                        "name": "bucket_id",
                        "prompt": "Bucket",
                        "required": true,
                        "readOnly": true,
                        "type": "select"
                    },
                    {
                        "hea": {
                            "section": "collaborators",
                            "optionsFromUrl": {
                                "path": "people/?excludesystem=yes",
                                "value": "id",
                                "text": "display_name"
                            },
                            "cardinality": "multiple"
                        },
                        "name": "collaborator_ids",
                        "prompt": "Collaborators",
                        "required": true,
                        "readOnly": true,
                        "type": "select"
                    },
                    {
                        "hea": {
                            "section": "collaborators",
                            "display": false
                        },
                        "name": "type",
                        "required": true,
                        "readOnly": true,
                        "value": "heaobject.account.AWSAccountCollaborators"
                    },
                    {
                        "hea": {
                            "section": "collaborators",
                            "display": false
                        },
                        "name": "type_display_name",
                        "required": true,
                        "readOnly": true
                    },
                    {
                        "name": "owner",
                        "prompt": "Owner",
                        "required": true,
                        "type": "select",
                        "readOnly": true,
                        "hea": {
                            "optionsFromUrl": {
                                "path": "people/",
                                "value": "id",
                                "text": "display_name"
                            }
                        }
                    },
                    {
                        "name": "created",
                        "prompt": "Created",
                        "readOnly": true,
                        "hea": {
                            "type": "datetime"
                        }
                    },
                    {
                        "name": "modified",
                        "prompt": "Modified",
                        "readOnly": true,
                        "hea": {
                            "type": "datetime"
                        }
                    },
                    {
                        "name": "source",
                        "prompt": "Source",
                        "readOnly": true
                    }
                ]
            },
            {
                "name": "heaserver-accounts-awsaccount-get-open-choices",
                "description": "Open this account",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "Open"
            },
            {
                "name": "heaserver-accounts-awsaccount-get-create-choices",
                "description": "Create a new object in this account",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "New"
            },
            {
                "name": "heaserver-accounts-awsaccount-create-bucket",
                "description": "Create a new bucket",
                "type": "safe",
                "action": "read",
                "target": "app read cj",
                "prompt": "New Bucket"
            },
            {
                "name": "heaserver-accounts-awsaccount-open-storage",
                "description": "Open S3 storage",
                "type": "safe",
                "action": "read",
                "target": "app read cj",
                "prompt": "Open"
            },
            {
                "name": "heaserver-accounts-awsaccount-open-buckets",
                "description": "Open S3 buckets",
                "type": "safe",
                "action": "read",
                "target": "app read cj",
                "prompt": "Open"
            },
            {
                "name": "heaserver-accounts-awsaccount-new-bucket-form",
                "description": "Create a new bucket",
                "type": "unsafe",
                "target": "item cj-template add",
                "action": "update",
                "prompt": "New Folder",
                "inputs": [
                    {
                        "name": "display_name",
                        "prompt": "Name",
                        "required": true,
                        "pattern": "^(cb-[a-z][a-z0-9\\.\\-]{0,})$"
                    },
                    {
                        "name": "type",
                        "prompt": "Type",
                        "value": "heaobject.bucket.AWSBucket",
                        "required": true,
                        "readOnly": true,
                        "hea": {
                            "display": false
                        }
                    },
                    {
                        "name": "region",
                        "prompt": "Region",
                        "required": true,
                        "type": "select",
                        "suggest": [
                            {"value": "us-east-2", "text": "US East (Ohio)"},
                            {"value": "us-east-1", "text": "US East (N. Virginia)"},
                            {"value": "us-west-1", "text": "US West (N. California)"},
                            {"value": "us-west-2", "text": "US West (Oregon)"},
                            {"value": "af-south-1", "text": "Africa (Cape Town)"},
                            {"value": "ap-east-1", "text": "Asia Pacific (Hong Kong)"},
                            {"value": "ap-southeast-3", "text": "Asia Pacific (Jakarta)"},
                            {"value": "ap-south-1", "text": "Asia Pacific (Mumbai)"},
                            {"value": "ap-northeast-3", "text": "Asia Pacific (Osaka)"},
                            {"value": "ap-northeast-2", "text": "Asia Pacific (Seoul)"},
                            {"value": "ap-southeast-1", "text": "Asia Pacific (Singapore)"},
                            {"value": "ap-southeast-2", "text": "Asia Pacific (Sydney)"},
                            {"value": "ap-northeast-1", "text": "Asia Pacific (Tokyo)"},
                            {"value": "ca-central-1", "text": "Canada (Central)"},
                            {"value": "eu-central-1", "text": "Europe (Frankfurt)"},
                            {"value": "eu-west-1", "text": "Europe (Ireland)"},
                            {"value": "eu-west-2", "text": "Europe (London)"},
                            {"value": "eu-south-1", "text": "Europe (Milan)"},
                            {"value": "eu-west-3", "text": "Europe (Paris)"},
                            {"value": "eu-north-1", "text": "Europe (Stockholm)"},
                            {"value": "me-south-1", "text": "Middle East (Bahrain)"},
                            {"value": "sa-east-1", "text": "South America (SÃ£o Paulo)"}
                        ],
                        "value": "us-east-1"
                    },
                    {
                        "name": "versioned",
                        "prompt": "Versioned",
                        "required": true,
                        "type": "select",
                        "suggest": [
                            {"value":  "true", "text":  "True"},
                            {"value":  "false", "text":  "False"}
                        ],
                        "value": "true"
                    },
                    {
                        "name": "instructions1",
                        "hea": {
                            "type": "text-display"
                        },
                        "value": "All bucket names must begin with \"cb\" followed by the first initial and beginning four characters of the owner's last name."
                    },
                    {
                        "name": "instructions2",
                        "hea": {
                            "type": "text-display"
                        },
                        "value": "For more information, see <a href=\"https://docs.aws.amazon.com/AmazonS3/latest/userguide/bucketnamingrules.html\" target=\"_blank\">Bucket Naming Rules</a>."
                    }
                ]
            }
        ]
    }
}
