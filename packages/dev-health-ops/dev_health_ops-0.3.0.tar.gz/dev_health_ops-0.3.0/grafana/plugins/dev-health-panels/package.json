{
  "name": "dev-health-panels",
  "version": "0.1.1-beta.1",
  "description": "Opinionated developer health panels for grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "playwright test",
    "server": "docker-compose up --build",
    "validate": "npx @grafana/plugin-validator@latest -analyzer=metadatavalid",
    "sign": "npx --yes @grafana/sign-plugin@latest --rootUrls https://chrisgeorge.grafana.net/"
  },
  "author": "Devhealth",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisgeo/dev-health-ops.git",
    "directory": "grafana/plugins/dev-health-panels"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/plugin-e2e": "^3.1.0",
    "@grafana/plugin-meta-extractor": "^0.11.0",
    "@grafana/tsconfig": "^2.0.1",
    "@playwright/test": "^1.41.2",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@swc/core": "^1.15.8",
    "@swc/helpers": "^0.5.18",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^25.0.3",
    "@types/react-router-dom": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-webpack-plugin": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "glob": "^13.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.7.4",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.97.1",
    "sass-loader": "16.0.6",
    "style-loader": "4.0.0",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^6.0.1",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@emotion/css": "11.13.5",
    "@grafana/data": "12.3.1",
    "@grafana/runtime": "12.3.1",
    "@grafana/schema": "12.3.1",
    "@grafana/ui": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "2.8.1"
  },
  "packageManager": "npm@11.6.2"
}
