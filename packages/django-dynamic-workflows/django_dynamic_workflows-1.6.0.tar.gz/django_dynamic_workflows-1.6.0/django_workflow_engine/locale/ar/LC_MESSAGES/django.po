msgid "Role-based Approval"
msgstr "موافقة قائمة على الدور"

msgid "User-specific Approval"
msgstr "موافقة خاصة بالمستخدم"

msgid "Self Approval"
msgstr "موافقة ذاتية"

msgid "Team Head Approval"
msgstr "موافقة رئيس الفريق"

msgid "Department Head Approval"
msgstr "موافقة رئيس القسم"

msgid "Any user with role can approve"
msgstr "أي مستخدم لديه الدور يمكنه الموافقة"

msgid "All users with role must approve"
msgstr "يجب أن توافق عليه جميع المستخدمين الذين لديهم الدور"

msgid "Rotate approval among role users"
msgstr "تدوير الموافقة بين مستخدمي الدور"

msgid "Workflow Approved"
msgstr "تمت الموافقة على سير العمل"

msgid "Workflow Rejected"
msgstr "تم رفض سير العمل"

msgid "Workflow Update"
msgstr "تحديث سير العمل"

msgid "No workflow attached to this object"
msgstr "لا يوجد سير عمل مرفق بهذا الكائن"

msgid "Workflow is not in progress"
msgstr "سير العمل ليس قيد التقدم"

msgid "Reason is required for rejection"
msgstr "السبب مطلوب للرفض"

msgid "Reason is required for resubmission"
msgstr "السبب مطلوب لإعادة التقديم"

msgid "Stage"
msgstr "المرحلة"

msgid "Pipeline"
msgstr "خط الأنابيب"

msgid "Active"
msgstr "نشط"

msgid "Inactive"
msgstr "غير نشط"

msgid "Completed"
msgstr "مكتمل"

msgid "In Progress"
msgstr "قيد التقدم"

msgid "Not Started"
msgstr "لم يبدأ"

msgid "Rejected"
msgstr "مرفوض"

msgid "Cancelled"
msgstr "ملغي"

msgid "Pending Approval"
msgstr "في انتظار الموافقة"

msgid "Approved"
msgstr "تمت الموافقة"

msgid "Delegated"
msgstr "مفوض"

msgid "Needs Resubmission"
msgstr "يتطلب إعادة التقديم"

msgid "Action '{action_name}' is not registered"
msgstr "الإجراء '{action_name}' غير مسجل"

msgid "Execution of action '{action_name}' failed: {error}"
msgstr "فشل تنفيذ الإجراء '{action_name}': {error}"

msgid "Workflow created - ID: {workflow_id}, Name: {name}"
msgstr "تم إنشاء سير العمل - المعرف: {workflow_id}، الاسم: {name}"

msgid "Workflow started for {obj_label}({obj_pk})"
msgstr "تم بدء سير العمل لـ {obj_label}({obj_pk})"

msgid "Workflow completed for {obj_label}({obj_pk})"
msgstr "تم إكمال سير العمل لـ {obj_label}({obj_pk})"

msgid "Stage '{stage}' approved for {obj_label}({obj_pk})"
msgstr "تمت الموافقة على المرحلة '{stage}' لـ {obj_label}({obj_pk})"

msgid "Stage '{stage}' rejected for {obj_label}({obj_pk})"
msgstr "تم رفض المرحلة '{stage}' لـ {obj_label}({obj_pk})"

msgid "Moved to next stage '{stage}' for {obj_label}({obj_pk})"
msgstr "تم النقل إلى المرحلة التالية '{stage}' لـ {obj_label}({obj_pk})"
