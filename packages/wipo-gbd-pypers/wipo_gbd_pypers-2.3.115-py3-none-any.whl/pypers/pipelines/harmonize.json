{
    "name" : "harmonize",
    "label": "ORI to GBD Format",

    "dag": {
        "nodes": {
            "manifests" : "harmonize.Manifests",
            "etl"       : "harmonize.ETLProcess",
            "index"     : "harmonize.Index",
            "notify"    : "harmonize.Notify"

        },
        "edges": [
            {
                "from"     : "manifests",
                "to"       : "etl",
                "bindings" : { "etl.manifest" : "manifests.manifest_list"}
            },
            {
                "from"     : "manifests",
                "to"       : "notify",
                "bindings" : { "notify.manifest" : "manifests.manifest_list"}
            },
            {
                "from"     : "etl",
                "to"       : "index",
                "bindings" : { "index.idx_files"  : "etl.idx_files",
                               "index.extraction_dir"  : "etl.extraction_dir" }
            },
            {
                "from"     : "index",
                "to"       : "notify",
                "bindings" : { "notify.flag" : "index.flag" }
            }
        ]
    },
    "config" : {
	    "steps": {
        }
    }
}

