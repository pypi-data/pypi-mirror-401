{
  "dashboard": {
    "id": null,
    "uid": "jira-routing-tests",
    "title": "JIRA Assistant - Routing Tests",
    "tags": ["claude-code", "jira", "routing", "tests"],
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 2,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "annotations": {
      "list": []
    },
    "panels": [
      {
        "id": 1,
        "title": "Routing Accuracy",
        "type": "stat",
        "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "red", "value": null },
                { "color": "yellow", "value": 50 },
                { "color": "green", "value": 80 }
              ]
            },
            "unit": "percent"
          },
          "overrides": []
        },
        "targets": [
          {
            "expr": "routing_accuracy_percent",
            "legendFormat": "Accuracy",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "Tests Passed",
        "type": "stat",
        "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 },
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null }
              ]
            },
            "unit": "short"
          },
          "overrides": []
        },
        "targets": [
          {
            "expr": "sum(traces_spanmetrics_calls_total{service=\"jira-assistant-routing-tests\", status_code=\"STATUS_CODE_OK\", span_name=~\"routing_test_TC.*\"})",
            "legendFormat": "Passed",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Tests Failed",
        "type": "stat",
        "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 },
