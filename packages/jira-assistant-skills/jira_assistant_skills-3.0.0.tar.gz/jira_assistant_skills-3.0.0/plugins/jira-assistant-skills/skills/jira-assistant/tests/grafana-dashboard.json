        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "blue", "value": null }
              ]
            },
            "unit": "short"
          },
          "overrides": []
        },
        "targets": [
          {
            "expr": "sum(traces_spanmetrics_calls_total{service=\"jira-assistant-routing-tests\", span_name=~\"routing_test_TC.*\"})",
            "legendFormat": "Total",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Test Sessions",
        "type": "stat",
        "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
