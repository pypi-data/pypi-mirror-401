        },
        "targets": [
          {
            "expr": "{service_name=\"claude-code\"}",
            "legendFormat": "",
            "refId": "A"
          }
        ]
      },
      {
        "id": 14,
        "title": "Test Details Table",
        "type": "table",
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 40 },
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "options": {
          "showHeader": true,
          "cellHeight": "sm",
          "footer": {
            "show": true,
            "reducer": ["sum"],
            "countRows": false,
            "fields": ["Value"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "custom": {
              "align": "auto",
              "cellOptions": { "type": "auto" },
              "inspect": false
            },
            "mappings": [
              { "options": { "STATUS_CODE_OK": { "color": "green", "index": 0, "text": "PASSED" } }, "type": "value" },
              { "options": { "STATUS_CODE_ERROR": { "color": "red", "index": 1, "text": "FAILED" } }, "type": "value" }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "text", "value": null }
              ]
            }
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "status_code" },
              "properties": [
                { "id": "custom.cellOptions", "value": { "type": "color-text" } },
                { "id": "displayName", "value": "Status" }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "span_name" },
              "properties": [
                { "id": "displayName", "value": "Test ID" }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "Value" },
              "properties": [
                { "id": "displayName", "value": "Count" }
              ]
            }
          ]
        },
        "transformations": [
          {
            "id": "sortBy",
            "options": {
              "fields": {},
              "sort": [{ "field": "span_name", "desc": false }]
            }
          }
        ],
        "targets": [
          {
            "expr": "traces_spanmetrics_calls_total{service=\"jira-assistant-routing-tests\", span_name=~\"routing_test_TC.*\"}",
            "legendFormat": "",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ]
      }
