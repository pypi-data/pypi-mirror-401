        "title": "Test Traces",
        "type": "traces",
        "gridPos": { "h": 10, "w": 24, "x": 0, "y": 20 },
        "datasource": { "type": "tempo", "uid": "tempo" },
        "options": {},
        "targets": [
          {
            "queryType": "traceqlSearch",
            "limit": 20,
            "tableType": "traces",
            "filters": [
              {
                "id": "service-name",
                "operator": "=",
                "scope": "resource",
                "tag": "service.name",
                "value": ["jira-assistant-routing-tests"],
                "valueType": "string"
              }
            ],
            "refId": "A"
          }
        ]
      },
      {
        "id": 13,
