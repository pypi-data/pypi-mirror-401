{
    "hosting": {
        "public": "out",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "redirects": [
            {
                "source": "/dashboard/docs/:path*",
                "destination": "/docs/:path*",
                "type": 301
            }
        ],
        "rewrites": [],
        "cleanUrls": true,
        "trailingSlash": false
    }
}