{
  "name": "{{ project_name }}",
  "type": "{{ project_type }}",
  "description": "{{ project_description }}",
  "version": "{{ project_version }}",
  "platform_url": "{{ platform_url }}",
  "author": "{{ author }}",
  "python_version": "{{ python_version }}",
  "entry_point": "src.main:app",
  "dependencies": {
    "fastapi": ">=0.104.0",
    "uvicorn[standard]": ">=0.24.0",
    "pydantic": ">=2.0.0",
    "httpx": ">=0.25.0",
    "dynaconf": ">=3.2.0",
    "pyyaml": ">=6.0.0"
  },
  "dev_dependencies": {
    "pytest": ">=7.0.0",
    "pytest-asyncio": ">=0.21.0",
    "black": ">=23.0.0",
    "flake8": ">=6.0.0",
    "mypy": ">=1.0.0"
  },
  "node_config": {
    "input_schema": {
      "type": "object",
      "properties": {
        "data": {
          "type": "object",
          "description": "输入数据"
        },
        "metadata": {
          "type": "object",
          "description": "元数据"
        }
      },
      "required": ["data"]
    },
    "output_schema": {
      "type": "object",
      "properties": {
        "result": {
          "type": "object",
          "description": "处理结果"
        },
        "status": {
          "type": "string",
          "description": "处理状态"
        },
        "metadata": {
          "type": "object",
          "description": "输出元数据"
        }
      },
      "required": ["result", "status"]
    },
    "endpoints": {
      "process": "/process",
      "health": "/health"
    }
  }
}