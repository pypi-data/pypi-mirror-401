{
  "name": "{{ project_name }}",
  "version": "{{ project_version }}",
  "description": "{{ project_description }}",
  "metadata": {
    "author": "{{ author }}",
    "created_at": "2023-12-17T00:00:00Z"
  },
  "config": {
    "max_parallel_nodes": 4,
    "timeout": 300,
    "retry_policy": {
      "max_retries": 3,
      "retry_delay": 1.0
    }
  },
  "nodes": [
    {
      "id": "input",
      "type": "InputNode",
      "name": "数据输入",
      "description": "接收外部输入数据",
      "config": {
        "schema": {
          "type": "object",
          "properties": {
            "data": {"type": "object"},
            "metadata": {"type": "object"}
          },
          "required": ["data"]
        }
      },
      "position": {"x": 100, "y": 100}
    },
    {
      "id": "validate",
      "type": "ValidationNode",
      "name": "数据验证",
      "description": "验证输入数据的有效性",
      "config": {
        "validation_rules": [
          {"field": "data", "required": true},
          {"field": "data.message", "type": "string"}
        ]
      },
      "position": {"x": 300, "y": 100}
    },
    {
      "id": "process",
      "type": "CustomProcessNode",
      "name": "数据处理",
      "description": "执行主要的数据处理逻辑",
      "config": {
        "processing_mode": "standard",
        "parameters": {
          "transform_type": "uppercase",
          "add_timestamp": true
        }
      },
      "position": {"x": 500, "y": 100}
    },
    {
      "id": "enrich",
      "type": "EnrichmentNode",
      "name": "数据增强",
      "description": "为处理后的数据添加额外信息",
      "config": {
        "enrichment_sources": ["metadata", "context"],
        "merge_strategy": "append"
      },
      "position": {"x": 700, "y": 100}
    },
    {
      "id": "output",
      "type": "OutputNode",
      "name": "结果输出",
      "description": "输出最终处理结果",
      "config": {
        "output_format": "json",
        "include_metadata": true
      },
      "position": {"x": 900, "y": 100}
    }
  ],
  "edges": [
    {
      "id": "input_to_validate",
      "from": "input",
      "to": "validate",
      "condition": null,
      "data_mapping": {
        "input": "output"
      }
    },
    {
      "id": "validate_to_process",
      "from": "validate",
      "to": "process",
      "condition": {
        "type": "success",
        "expression": "validation_result == 'valid'"
      },
      "data_mapping": {
        "validated_data": "input_data"
      }
    },
    {
      "id": "process_to_enrich",
      "from": "process",
      "to": "enrich",
      "condition": null,
      "data_mapping": {
        "processed_data": "base_data"
      }
    },
    {
      "id": "enrich_to_output",
      "from": "enrich",
      "to": "output",
      "condition": null,
      "data_mapping": {
        "enriched_data": "final_data"
      }
    }
  ],
  "error_handling": {
    "default_strategy": "stop_on_error",
    "node_specific": {
      "validate": {
        "strategy": "skip_and_continue",
        "fallback_node": "output"
      }
    }
  }
}