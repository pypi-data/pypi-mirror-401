{
  "name": "{{ project_name }}",
  "type": "{{ project_type }}",
  "description": "{{ project_description }}",
  "version": "{{ project_version }}",
  "platform_url": "{{ platform_url }}",
  "author": "{{ author }}",
  "python_version": "{{ python_version }}",
  "entry_point": "src.main:app",
  "dependencies": {
    "fastapi": ">=0.104.0",
    "uvicorn[standard]": ">=0.24.0",
    "pydantic": ">=2.0.0",
    "httpx": ">=0.25.0",
    "networkx": ">=3.0",
    "celery": ">=5.3.0",
    "redis": ">=5.0.0",
    "dynaconf": ">=3.2.0",
    "pyyaml": ">=6.0.0"
  },
  "dev_dependencies": {
    "pytest": ">=7.0.0",
    "pytest-asyncio": ">=0.21.0",
    "black": ">=23.0.0",
    "flake8": ">=6.0.0",
    "mypy": ">=1.0.0"
  },
  "graph_config": {
    "workflow_file": "workflow.json",
    "max_parallel_nodes": 4,
    "default_timeout": 300,
    "retry_policy": {
      "max_retries": 3,
      "retry_delay": 1.0
    },
    "endpoints": {
      "execute": "/execute",
      "health": "/health",
      "workflow_info": "/workflow/info",
      "workflow_config": "/workflow/config"
    },
    "supported_node_types": [
      "InputNode",
      "OutputNode",
      "ValidationNode",
      "CustomProcessNode",
      "EnrichmentNode"
    ]
  }
}