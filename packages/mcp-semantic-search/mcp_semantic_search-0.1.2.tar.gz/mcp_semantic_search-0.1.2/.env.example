GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_EMBEDDING_MODEL=models/text-embedding-004
QDRANT_URL=http://qdrant:6333

# Semantic Search Chunk Configuration
# Maximum lines of code per chunk (default: 50, was: 150)
# Lower values = smaller responses, more chunks, higher storage usage
CHUNK_MAX_LINES=50
# Overlap lines between chunks (default: 10, was: 20)
# Provides context continuity between chunks
CHUNK_OVERLAP_LINES=10
# Minimum lines for a valid chunk (default: 5, was: 10)
# Chunks smaller than this are merged with previous chunk
CHUNK_MIN_LINES=5
