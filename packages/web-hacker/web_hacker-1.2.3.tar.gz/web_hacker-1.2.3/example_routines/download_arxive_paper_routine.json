{
    "name": "Download Arxiv Paper",
    "description": "Download arxive paper as a PDF",
    "operations": [
        {
            "type": "download",
            "endpoint": {
                "headers": {},
                "method": "GET",
                "credentials": "omit",
                "url": "https://arxiv.org/pdf/\"{{paper_id}}\""
            },
            "filename": "\"{{paper_id}}\".pdf"
        }
    ],
    "incognito": true,
    "parameters": [
        {
            "examples": [
                "1706.03762"
            ],
            "min_length": 1,
            "name": "paper_id",
            "description": "paper id on arxiv",
            "type": "string",
            "required": true,
            "max_length": 200
        }
    ]
}