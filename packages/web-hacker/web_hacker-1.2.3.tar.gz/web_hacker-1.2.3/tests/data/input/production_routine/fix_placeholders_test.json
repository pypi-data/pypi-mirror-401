{
  "name": "fix_placeholders_test_routine",
  "description": "Test routine for fix_placeholders method.",
  "operations": [
    {
      "type": "navigate",
      "url": "https://example.com/users/{{user_id}}/profile"
    },
    {
      "type": "fetch",
      "endpoint": {
        "url": "https://api.example.com/data?q={{search_term}}",
        "method": "POST",
        "headers": {
          "Content-Type": "application/json",
          "X-User-Name": "{{user_name}}",
          "X-User-Age": "{{user_age}}",
          "X-Session-ID": "{{sessionStorage:session.id}}",
          "X-Existing-Quote": "\"{{user_name}}\""
        },
        "body": {
          "user": {
            "name": "{{user_name}}",
            "age": "{{user_age}}",
            "role": "{{user_role}}",
            "meta": "{{sessionStorage:meta.data}}"
          },
          "query": "{{search_term}}",
          "quoted_already": "\"{{search_term}}\""
        }
      },
      "session_storage_key": "result"
    },
    {
      "type": "return",
      "session_storage_key": "result"
    }
  ],
  "parameters": [
    {
      "name": "user_id",
      "type": "string",
      "description": "User ID"
    },
    {
      "name": "search_term",
      "type": "string",
      "description": "Search term"
    },
    {
      "name": "user_name",
      "type": "string",
      "description": "User name"
    },
    {
      "name": "user_age",
      "type": "integer",
      "description": "User age"
    },
    {
      "name": "user_role",
      "type": "enum",
      "enum_values": ["admin", "user"],
      "description": "User role"
    }
  ]
}

