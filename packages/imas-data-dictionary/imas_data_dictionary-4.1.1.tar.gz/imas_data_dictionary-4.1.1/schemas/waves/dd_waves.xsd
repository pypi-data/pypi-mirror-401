			<xs:element name="psi">
				<xs:annotation>
					<xs:documentation>Poloidal magnetic flux</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
						<coordinate2>1...N</coordinate2>
						<coordinate1_same_as>../r</coordinate1_same_as>
						<coordinate2_same_as>../r</coordinate2_same_as>
						<units>Wb</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="volume">
				<xs:annotation>
					<xs:documentation>Volume enclosed inside the magnetic surface</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>1...N</coordinate1>
						<coordinate2>1...N</coordinate2>
						<coordinate1_same_as>../r</coordinate1_same_as>
						<coordinate2_same_as>../r</coordinate2_same_as>
