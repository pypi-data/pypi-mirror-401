		</xs:annotation>
		<xs:sequence>
			<xs:element name="type" type="identifier_dynamic_aos3">
				<xs:annotation>
					<xs:documentation>Grid type: index=0: Rectangular grid in the (R,Z) coordinates; index=1: Rectangular grid in the (radial, theta_geometric) coordinates; index=2: Rectangular grid in the (radial, theta_straight) coordinates. index=3: unstructured grid.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="r">
				<xs:annotation>
					<xs:documentation>Major radius</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>1...N</coordinate1>
						<coordinate2>1...N</coordinate2>
						<units>m</units>
