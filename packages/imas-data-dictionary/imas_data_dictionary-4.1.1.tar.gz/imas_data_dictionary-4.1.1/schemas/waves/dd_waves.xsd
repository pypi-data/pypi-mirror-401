					<xs:group ref="FLT_2D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="z">
				<xs:annotation>
					<xs:documentation>Height</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>1...N</coordinate1>
						<coordinate2>1...N</coordinate2>
						<coordinate1_same_as>../r</coordinate1_same_as>
						<coordinate2_same_as>../r</coordinate2_same_as>
						<units>m</units>
