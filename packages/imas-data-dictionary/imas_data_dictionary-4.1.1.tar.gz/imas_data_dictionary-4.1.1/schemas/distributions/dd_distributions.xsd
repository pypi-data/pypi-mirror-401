<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="../utilities/dd_support.xsd"/>
	<xs:complexType name="distributions_d_source_identifier">
		<xs:annotation>
			<xs:documentation>Identifier of the source/sink term (wave or particle source process)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="type" type="identifier_dynamic_aos3">
				<xs:annotation>
					<xs:documentation>Type of the source term. Index  = 1 for a wave, index = 2 for a particle source process</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wave_index">
				<xs:annotation>
					<xs:documentation>Index into distribution/wave</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="process_index">
				<xs:annotation>
					<xs:documentation>Index into distribution/process</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="distributions_d_global_quantities_thermalized">
		<xs:annotation>
			<xs:documentation>Global quantities for thermalization source/sinks</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="particles">
				<xs:annotation>
					<xs:documentation>Source rate of thermal particles due to the thermalization of fast particles</xs:documentation>
					<xs:appinfo>
						<units>s^-1</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="power">
				<xs:annotation>
					<xs:documentation>Power input to the thermal particle population due to the thermalization of fast particles</xs:documentation>
					<xs:appinfo>
						<units>W</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="torque">
				<xs:annotation>
					<xs:documentation>Torque input to the thermal particle population due to the thermalization of fast particles</xs:documentation>
					<xs:appinfo>
						<units>N.m</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="distributions_d_global_quantities_source">
		<xs:annotation>
			<xs:documentation>Global quantities for a given source/sink term</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="identifier" type="distributions_d_source_identifier">
				<xs:annotation>
					<xs:documentation>Identifier of the wave or particle source process, defined respectively in distribution/wave or distribution/process</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="particles">
				<xs:annotation>
					<xs:documentation>Particle source rate</xs:documentation>
					<xs:appinfo>
						<units>s^-1</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="power">
				<xs:annotation>
					<xs:documentation>Total power of the source</xs:documentation>
					<xs:appinfo>
						<units>W</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="torque_phi">
				<xs:annotation>
					<xs:documentation>Total toroidal torque of the source</xs:documentation>
					<xs:appinfo>
						<units>N.m</units>
						<type>dynamic</type>
						<change_nbc_version>3.42.0</change_nbc_version>
						<change_nbc_description>leaf_renamed</change_nbc_description>
						<change_nbc_previous_name>torque_tor</change_nbc_previous_name>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="distributions_d_global_quantities_collisions_ion_state">
		<xs:annotation>
			<xs:documentation>Global quantities for collisions with a given ion species state</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="z_min">
				<xs:annotation>
					<xs:documentation>Minimum Z of the charge state bundle (z_min = z_max = 0 for a neutral)</xs:documentation>
					<xs:appinfo>
						<units>e</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="z_max">
				<xs:annotation>
					<xs:documentation>Maximum Z of the charge state bundle (equal to z_min if no bundle)</xs:documentation>
					<xs:appinfo>
						<units>e</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="name">
				<xs:annotation>
					<xs:documentation>String identifying charge state (e.g. C+, C+2 , C+3, C+4, C+5, C+6, ...)</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<change_nbc_version>3.42.0</change_nbc_version>
						<change_nbc_description>leaf_renamed</change_nbc_description>
						<change_nbc_previous_name>label</change_nbc_previous_name>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="electron_configuration">
				<xs:annotation>
					<xs:documentation>Configuration of atomic orbitals of this state, e.g. 1s2-2s1</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="vibrational_level">
				<xs:annotation>
					<xs:documentation>Vibrational level (can be bundled)</xs:documentation>
					<xs:appinfo>
						<units>e</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="vibrational_mode">
				<xs:annotation>
					<xs:documentation>Vibrational mode of this state, e.g. "A_g". Need to define, or adopt a standard nomenclature.</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="power_thermal">
				<xs:annotation>
					<xs:documentation>Collisional power to the thermal particle population</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>W</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="power_fast">
				<xs:annotation>
					<xs:documentation>Collisional power to the fast particle population</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>W</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="torque_thermal_phi">
				<xs:annotation>
					<xs:documentation>Collisional toroidal torque to the thermal particle population</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>N.m</units>
						<change_nbc_version>3.42.0</change_nbc_version>
						<change_nbc_description>leaf_renamed</change_nbc_description>
						<change_nbc_previous_name>torque_thermal_tor</change_nbc_previous_name>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="torque_fast_phi">
				<xs:annotation>
					<xs:documentation>Collisional toroidal torque to the fast particle population</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>N.m</units>
						<change_nbc_version>3.42.0</change_nbc_version>
						<change_nbc_description>leaf_renamed</change_nbc_description>
						<change_nbc_previous_name>torque_fast_tor</change_nbc_previous_name>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="distributions_d_global_quantities_collisions_ion">
		<xs:annotation>
			<xs:documentation>Global quantities for collisions with a given ion species</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="element" type="plasma_composition_neutral_element" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>List of elements forming the atom or molecule</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="z_ion">
				<xs:annotation>
					<xs:documentation>Ion charge (of the dominant ionization state; lumped ions are allowed).</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>e</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="name">
				<xs:annotation>
					<xs:documentation>String identifying the species (e.g. H+, D+, T+, He+2, C+, D2, DT, CD4, ...)</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<change_nbc_version>3.42.0</change_nbc_version>
						<change_nbc_description>leaf_renamed</change_nbc_description>
						<change_nbc_previous_name>label</change_nbc_previous_name>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="neutral_index">
				<xs:annotation>
					<xs:documentation>Index of the corresponding neutral species in the ../../neutral array</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="power_thermal">
				<xs:annotation>
					<xs:documentation>Collisional power to the thermal particle population</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>W</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="power_fast">
				<xs:annotation>
					<xs:documentation>Collisional power to the fast particle population</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>W</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="torque_thermal_phi">
				<xs:annotation>
					<xs:documentation>Collisional toroidal torque to the thermal particle population</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>N.m</units>
						<change_nbc_version>3.42.0</change_nbc_version>
						<change_nbc_description>leaf_renamed</change_nbc_description>
						<change_nbc_previous_name>torque_thermal_tor</change_nbc_previous_name>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="torque_fast_phi">
				<xs:annotation>
					<xs:documentation>Collisional toroidal torque to the fast particle population</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>N.m</units>
						<change_nbc_version>3.42.0</change_nbc_version>
						<change_nbc_description>leaf_renamed</change_nbc_description>
						<change_nbc_previous_name>torque_fast_tor</change_nbc_previous_name>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="multiple_states_flag">
				<xs:annotation>
					<xs:documentation>Multiple state calculation flag : 0-Only one state is considered; 1-Multiple states are considered and are described in the state structure</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="state" type="distributions_d_global_quantities_collisions_ion_state" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Collisional exchange with the various states of the ion species (ionization, energy, excitation, ...)</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="distributions_d_global_quantities_collisions_electrons">
		<xs:annotation>
			<xs:documentation>Global quantities for collisions with electrons</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="power_thermal">
				<xs:annotation>
					<xs:documentation>Collisional power to the thermal particle population</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>W</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="power_fast">
				<xs:annotation>
					<xs:documentation>Collisional power to the fast particle population</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>W</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="torque_thermal_phi">
				<xs:annotation>
					<xs:documentation>Collisional toroidal torque to the thermal particle population</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>N.m</units>
						<change_nbc_version>3.42.0</change_nbc_version>
						<change_nbc_description>leaf_renamed</change_nbc_description>
						<change_nbc_previous_name>torque_thermal_tor</change_nbc_previous_name>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="torque_fast_phi">
				<xs:annotation>
					<xs:documentation>Collisional toroidal torque to the fast particle population</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>N.m</units>
						<change_nbc_version>3.42.0</change_nbc_version>
						<change_nbc_description>leaf_renamed</change_nbc_description>
						<change_nbc_previous_name>torque_fast_tor</change_nbc_previous_name>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="distributions_d_global_quantities_collisions">
		<xs:annotation>
			<xs:documentation>Global quantities for collisions</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="electrons" type="distributions_d_global_quantities_collisions_electrons">
				<xs:annotation>
					<xs:documentation>Collisional exchange with electrons</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ion" type="distributions_d_global_quantities_collisions_ion" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Collisional exchange with the various ion species</xs:documentation>
					<xs:appinfo>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="distributions_d_global_quantities">
		<xs:annotation>
			<xs:documentation>Global quantities from the distribution, for a given time slice</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="particles_n">
				<xs:annotation>
					<xs:documentation>Number of particles in the distribution, i.e. the volume integral of the density (note: this is the number of real particles and not markers)</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>1</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="particles_fast_n">
				<xs:annotation>
					<xs:documentation>Number of fast particles in the distribution, i.e. the volume integral of the density (note: this is the number of real particles and not markers)</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>1</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="energy">
				<xs:annotation>
					<xs:documentation>Total energy in the distribution</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>J</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="energy_fast">
				<xs:annotation>
					<xs:documentation>Total energy of the fast particles in the distribution</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>J</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="energy_fast_parallel">
				<xs:annotation>
					<xs:documentation>Parallel energy of the fast particles in the distribution</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>J</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="torque_tor_j_radial">
				<xs:annotation>
					<xs:documentation>Toroidal torque due to radial currents</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>N.m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="current_phi">
				<xs:annotation>
					<xs:documentation>Toroidal current driven by the distribution</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>A</units>
						<change_nbc_version>3.42.0</change_nbc_version>
						<change_nbc_description>leaf_renamed</change_nbc_description>
						<change_nbc_previous_name>current_tor</change_nbc_previous_name>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="power_first_orbit">
				<xs:annotation>
					<xs:documentation>Power loss due to first orbit losses</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>W</units>
						<introduced_after_version>4.0.0</introduced_after_version>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="power_rotation">
				<xs:annotation>
					<xs:documentation>Power going to plasma rotation. This power goes into sustaining the plasma rotation rather than heating the plasma. This is given because the injection energy in the rotating plasma frame is (assuming rotation in the same direction of the NB injection) lower than in the lab-frame.</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>W</units>
						<introduced_after_version>4.0.0</introduced_after_version>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="power_charge_exchange">
				<xs:annotation>
					<xs:documentation>Power loss due to Charge Exchange loss of fast ions</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>W</units>
						<introduced_after_version>4.0.0</introduced_after_version>
