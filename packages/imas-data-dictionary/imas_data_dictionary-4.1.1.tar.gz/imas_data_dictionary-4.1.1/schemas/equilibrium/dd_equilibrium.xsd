				<xs:annotation>
					<xs:documentation>External voltage, i.e. time derivative of psi_external_average (with a minus sign : - d_psi_external_average/d_time)</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<units>V</units>
						<cocos_label_transformation>ip_like</cocos_label_transformation>
