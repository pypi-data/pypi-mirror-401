						<type>dynamic</type>
						<units>ohm</units>
						<introduced_after_version>3.37.2</introduced_after_version>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
