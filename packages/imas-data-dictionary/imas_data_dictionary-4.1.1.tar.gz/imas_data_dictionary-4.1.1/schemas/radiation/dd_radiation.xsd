				<xs:annotation>
					<xs:documentation>Power emitted by electrons</xs:documentation>
					<xs:appinfo>
						<units>W</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
