{
  "title": "群通知实体",
  "description": "群通知实体",
  "oneOf": [
    {
      "title": "用户入群请求",
      "description": "用户入群请求",
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "join_request"
          ]
        },
        "group_id": {
          "description": "群号",
          "type": "integer",
          "minimum": -9007199254740991,
          "maximum": 9007199254740991
        },
        "notification_seq": {
          "description": "通知序列号",
          "type": "integer",
          "minimum": -9007199254740991,
          "maximum": 9007199254740991
        },
        "is_filtered": {
          "description": "请求是否被过滤（发起自风险账户）",
          "type": "boolean"
        },
        "initiator_id": {
          "description": "发起者 QQ 号",
          "type": "integer",
          "minimum": -9007199254740991,
          "maximum": 9007199254740991
        },
        "state": {
          "description": "请求状态",
          "type": "string",
          "enum": [
            "pending",
            "accepted",
            "rejected",
            "ignored"
          ]
        },
        "operator_id": {
          "description": "处理请求的管理员 QQ 号",
          "nullable": true,
          "type": "integer",
          "minimum": -9007199254740991,
          "maximum": 9007199254740991
        },
        "comment": {
          "description": "入群请求附加信息",
          "type": "string"
        }
      },
      "required": [
        "type",
        "group_id",
        "notification_seq",
        "is_filtered",
        "initiator_id",
        "state",
        "comment"
      ]
    },
