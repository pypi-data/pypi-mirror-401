# Nova Agent - Production Environment
# 프로덕션 환경 설정

# Gateway WebSocket URL
NOVA_GATEWAY_URL=wss://agent-gw.qanova.kr/ws/agent

# Frontend URL (Agent 등록 페이지)
NOVA_FRONTEND_URL=https://qanova.kr

# Agent 등록 경로
NOVA_AGENT_REGISTER_PATH=/agent/register

# 로그 레벨 (DEBUG, INFO, WARNING, ERROR)
NOVA_LOG_LEVEL=INFO

# 브라우저 설정
NOVA_HEADLESS=true
NOVA_VIEWPORT_WIDTH=1920
NOVA_VIEWPORT_HEIGHT=1080

# Heartbeat 간격 (초)
NOVA_HEARTBEAT_INTERVAL=30

# 연결 설정
NOVA_CONNECTION_TIMEOUT=10
NOVA_RECONNECT_DELAY=5
NOVA_MAX_RECONNECT_ATTEMPTS=10
