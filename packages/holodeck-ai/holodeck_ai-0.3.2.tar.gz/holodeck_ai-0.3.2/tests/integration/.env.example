# Integration Test Environment Variables
# Copy this file to .env and fill in your actual API keys

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL_NAME=gpt-4o-mini

# Azure OpenAI Configuration
AZURE_OPENAI_API_KEY=your-azure-openai-api-key-here
AZURE_OPENAI_ENDPOINT=https://your-instance.openai.azure.com
AZURE_OPENAI_DEPLOYMENT_NAME=gpt-4o

# Anthropic Configuration
ANTHROPIC_API_KEY=your-anthropic-api-key-here
ANTHROPIC_MODEL_NAME=claude-3-5-sonnet-20241022

# Ollama Configuration (local LLM)
OLLAMA_ENDPOINT=http://truenas.home:11434
OLLAMA_EMBEDDING_MODEL=nomic-embed-text:latest
OLLAMA_COMPLETION_MODEL=gpt-oss:20b
# Embedding dimensions for nomic-embed-text (768)
OLLAMA_EMBEDDING_DIMENSIONS=768

# Test Configuration
# Set to "true" to skip tests that require real API keys
SKIP_LLM_INTEGRATION_TESTS=false
