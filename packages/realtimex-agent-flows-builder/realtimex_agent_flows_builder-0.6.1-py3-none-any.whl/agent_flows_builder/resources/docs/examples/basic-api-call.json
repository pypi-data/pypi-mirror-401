{
  "name": "Basic API Call Example",
  "description": "Simple GET request to fetch user data from JSONPlaceholder API",
  "uuid": "basic-api-call-example",
  "active": true,
  "steps": [
    {
      "type": "start",
      "config": {
        "variables": [
          {
            "name": "endpoint",
            "value": "users/1",
            "type": "string",
            "description": "The API endpoint to call"
          }
        ]
      }
    },
    {
      "type": "apiCall",
      "config": {
        "url": "https://jsonplaceholder.typicode.com/{{ $endpoint }}",
        "method": "GET",
        "responseVariable": "user_data",
        "timeout": 30,
        "maxRetries": 2
      }
    }
  ]
}