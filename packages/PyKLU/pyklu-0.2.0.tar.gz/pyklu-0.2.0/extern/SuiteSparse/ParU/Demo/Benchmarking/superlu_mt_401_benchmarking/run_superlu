# A generic script for running SuperLU_MT on the test RB_matrices used
# for the ParU ACM TOMS submission.  First use ../get_RB_matrices
# to download the test matrices to your /tmp/RB_matrices folder.

echo "Matrix: TSOPF_RS_b39_c30.rb,"
~/superlu_mt/build/EXAMPLE/pdlinsolx_paru < /tmp/RB_matrices/TSOPF_RS_b39_c30/TSOPF_RS_b39_c30.rb

