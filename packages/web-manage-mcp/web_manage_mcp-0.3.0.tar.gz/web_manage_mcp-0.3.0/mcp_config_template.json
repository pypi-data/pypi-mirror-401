{
  "mcpServers": {
    "web-manage-mcp": {
      "command": "uvx",
      "args": [
        "web-manage-mcp"
      ],
      "env": {
        "MCP_HOST": "127.0.0.1",
        "MCP_PORT": "8000",
        "MCP_COOKIE_AUTH_NAME": "<填写您的Cookie认证名称>",
        "MCP_COOKIE_AUTH_TOKEN": "<填写您的Cookie认证token值>"
      }
    }
  }
}