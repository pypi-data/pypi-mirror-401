# Example secrets template - copy to secrets.yaml and fill in values
# DO NOT commit secrets.yaml to version control!

apiVersion: v1
kind: Secret
metadata:
  name: gaik-demo-api-auth
type: Opaque
stringData:
  API_USERNAME: "gaik-demo-user"
  API_PASSWORD: "your-secure-password-here"
---
apiVersion: v1
kind: Secret
metadata:
  name: gaik-demo-api-keys
type: Opaque
stringData:
  OPENAI_API_KEY: "sk-your-openai-api-key"
