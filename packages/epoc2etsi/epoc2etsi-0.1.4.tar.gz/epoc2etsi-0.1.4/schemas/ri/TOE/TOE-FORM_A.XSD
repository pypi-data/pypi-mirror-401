<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
  targetNamespace="http://data.europa.eu/edm/1/ns/forms/TOE-FORM_A.XSD#"
  xmlns="http://data.europa.eu/edm/1/ns/forms/TOE-FORM_A.XSD#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:udt="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
  xmlns:civil="http://data.europa.eu/edm/1/ns/civil"
  xmlns:toe="http://data.europa.eu/edm/1/ns/toe"
  elementFormDefault="qualified"
  version="1.0">

  <!-- ===== Imports======================================== -->
  <xsd:import namespace="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
              schemaLocation="../_external/UBL-2.1/xsdrt/common/UBL-UnqualifiedDataTypes-2.1.xsd"/>
  <xsd:import namespace="http://data.europa.eu/edm/1/ns/civil" schemaLocation="../_common/civil-common.xsd"/>
  <xsd:import namespace="http://data.europa.eu/edm/1/ns/toe" schemaLocation="./toe-common.xsd"/>
  <!-- ===================================================== -->

  <xsd:element name="Toe_FormA" type="Toe_FormA_Type"/>

  <xsd:complexType name="Toe_FormA_Type">
    <xsd:sequence>
      <xsd:element name="Section1" type="udt:TextType"/>
      <xsd:element name="Section2" type="civil:Civil_TransmissionEntity_Type"/>
      <xsd:element name="Section3" type="civil:Civil_TransmissionEntity_Type" minOccurs="0"/>
      <xsd:element name="Section4" type="civil:Civil_EntitiesInfo_Type"/>
      <xsd:element name="Section5" type="civil:Civil_EntitiesInfo_Type"/>
      <xsd:element name="Section6" type="civil:Civil_EntitiesInfo_Type"/>
      <xsd:element name="Section7" type="civil:Civil_EntitiesInfo_Type"/>
      <xsd:element name="Section8" type="Toe_FormA_PartiesPresenceAndParticipation_Type"/>
      <xsd:element name="Section9" type="Toe_FormA_RepresentativesPresenceAndParticipationContainer_Type" minOccurs="0"/>
      <xsd:element name="Section10" type="udt:TextType"/>
      <xsd:element name="Section11" type="Toe_FormA_TakingOfEvidenceToBeCarriedOut_Type"/>
      <xsd:element name="Section12" type="Toe_FormA_RequestExecution_Type"/>
      <xsd:element name="Section13" type="Toe_FormA_ReasonForNotTransmitting_Type"/>
      <xsd:element name="Section14" type="civil:Civil_DoneAtDate_Type"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormA_PartiesPresenceAndParticipation_Type">
    <xsd:sequence>
      <xsd:element name="PartiesPresent" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="PartiesParticipationRequested" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="Languages" type="civil:Civil_EuLanguages_Type"/>
      <xsd:element name="LanguageOther" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormA_RepresentativesPresenceAndParticipationContainer_Type">
    <xsd:sequence>
      <xsd:element name="Value" type="Toe_FormA_RepresentativesPresenceAndParticipation_Type" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormA_RepresentativesPresenceAndParticipation_Type">
    <xsd:sequence>
      <xsd:element name="RepresentativesPresent" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="RepresentativesParticipationRequested"
                   type="Toe_FormA_RepresentativesParticipationRequestedContainer_Type" minOccurs="0"/>
      <xsd:element name="Languages" type="civil:Civil_EuLanguages_Type"/>
      <xsd:element name="LanguageOther" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormA_RepresentativesParticipationRequestedContainer_Type">
    <xsd:sequence>
      <xsd:element name="Value" type="toe:Toe_NameTitleFunctionTask_Type" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormA_RequestExecution_Type">
    <xsd:sequence>
      <xsd:element name="InAccordanceWithSpecialProcedure" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="ByTheUseOfCommunicationsTechnology" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="NecessaryInformation" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormA_TakingOfEvidenceToBeCarriedOut_Type">
    <xsd:sequence>
      <xsd:element name="Description" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="ExaminationOfWitnesses" type="Toe_FormA_ExaminationOfWitnessesContainer_Type" minOccurs="0"/>
      <xsd:element name="OtherTakingOfEvidence" type="Toe_FormA_OtherTakingOfEvidenceContainer_Type" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormA_ExaminationOfWitnessesContainer_Type">
    <xsd:sequence>
      <xsd:element name="Value" type="Toe_FormA_ExaminationOfWitness_Type" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormA_ExaminationOfWitness_Type">
    <xsd:sequence>
      <xsd:element name="ExaminationOfWitness" type="toe:Toe_ExaminationOfWitness_Type"/>
      <xsd:element name="Examination" type="Toe_FormA_Examination_Type" minOccurs="0"/>
      <xsd:element name="AnyOtherInformation" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:simpleType name="Toe_FormA_Examination_Type">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="UNDER_OATH"/>
      <xsd:enumeration value="ON_AFFIRMATION"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="Toe_FormA_OtherTakingOfEvidenceContainer_Type">
    <xsd:sequence>
      <xsd:element name="Value" type="Toe_FormA_OtherTakingOfEvidence_Type" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormA_OtherTakingOfEvidence_Type">
    <xsd:sequence>
      <xsd:element name="DocumentsToBeInspected" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="ObjectsToBeInspected" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormA_ReasonForNotTransmitting_Type">
    <xsd:sequence>
      <xsd:element name="DisruptionOfTheDecentralisedItSystem" type="udt:IndicatorType" minOccurs="0"/>
      <xsd:element name="ExceptionalCircumstances" type="udt:IndicatorType" minOccurs="0"/>
      <xsd:element name="NatureOfTheEvidence" type="udt:IndicatorType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

</xsd:schema>
