<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
  targetNamespace="http://data.europa.eu/edm/1/ns/forms/TOE-FORM_L.XSD#"
  xmlns="http://data.europa.eu/edm/1/ns/forms/TOE-FORM_L.XSD#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:udt="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
  xmlns:civil="http://data.europa.eu/edm/1/ns/civil"
  xmlns:toe="http://data.europa.eu/edm/1/ns/toe"
  elementFormDefault="qualified"
  version="1.0">

  <!-- ===== Imports======================================== -->
  <xsd:import namespace="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
              schemaLocation="../_external/UBL-2.1/xsdrt/common/UBL-UnqualifiedDataTypes-2.1.xsd"/>
  <xsd:import namespace="http://data.europa.eu/edm/1/ns/civil" schemaLocation="../_common/civil-common.xsd"/>
  <xsd:import namespace="http://data.europa.eu/edm/1/ns/toe" schemaLocation="./toe-common.xsd"/>
  <!-- ===================================================== -->

  <xsd:element name="Toe_FormL" type="Toe_FormL_Type"/>

  <xsd:complexType name="Toe_FormL_Type">
    <xsd:sequence>
      <xsd:element name="Section1To2" type="civil:Civil_ReferenceNumbers_Type"/>
      <xsd:element name="Section3" type="civil:Civil_TransmissionEntity_Type"/>
      <xsd:element name="Section4" type="civil:Civil_TransmissionEntity_Type" minOccurs="0"/>
      <xsd:element name="Section5" type="civil:Civil_EntitiesInfo_Type"/>
      <xsd:element name="Section6" type="civil:Civil_EntitiesInfo_Type"/>
      <xsd:element name="Section7" type="civil:Civil_EntitiesInfo_Type"/>
      <xsd:element name="Section8" type="civil:Civil_EntitiesInfo_Type"/>
      <xsd:element name="Section9" type="Toe_FormL_NameTitleFunctionTasks_Type"/>
      <xsd:element name="Section10" type="udt:TextType"/>
      <xsd:element name="Section11" type="Toe_FormL_TakingOfEvidenceToBeCarriedOut_Type"/>
      <xsd:element name="Section12" type="Toe_FormL_EvidenceBeTakenDirectlyAsSetOutInFormN_Type"/>
      <xsd:element name="Section13" type="civil:Civil_DoneAtDate_Type"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormL_NameTitleFunctionTasks_Type">
    <xsd:sequence>
      <xsd:element name="NameTitleFunctionTask" type="toe:Toe_NameTitleFunctionTask_Type" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormL_TakingOfEvidenceToBeCarriedOut_Type">
    <xsd:sequence>
      <xsd:element name="Description" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="ExaminationOfWitnesses" type="Toe_FormL_ExaminationOfWitnessesContainer_Type" minOccurs="0"/>
      <xsd:element name="OtherTakingOfEvidence" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormL_ExaminationOfWitnessesContainer_Type">
    <xsd:sequence>
      <xsd:element name="Value" type="toe:Toe_ExaminationOfWitness_Type" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormL_EvidenceBeTakenDirectlyAsSetOutInFormN_Type">
    <xsd:sequence>
      <xsd:element name="EvidenceBeTakenDirectlyAsSetOutInFormN" type="udt:IndicatorType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

</xsd:schema>
