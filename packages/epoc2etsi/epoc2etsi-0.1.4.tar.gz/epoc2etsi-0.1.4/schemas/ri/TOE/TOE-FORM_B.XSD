<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
  targetNamespace="http://data.europa.eu/edm/1/ns/forms/TOE-FORM_B.XSD#"
  xmlns="http://data.europa.eu/edm/1/ns/forms/TOE-FORM_B.XSD#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:udt="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
  xmlns:civil="http://data.europa.eu/edm/1/ns/civil"
  xmlns:toe="http://data.europa.eu/edm/1/ns/toe"
  elementFormDefault="qualified"
  version="1.0">

  <!-- ===== Imports======================================== -->
  <xsd:import namespace="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
              schemaLocation="../_external/UBL-2.1/xsdrt/common/UBL-UnqualifiedDataTypes-2.1.xsd"/>
  <xsd:import namespace="http://data.europa.eu/edm/1/ns/civil"
              schemaLocation="../_common/civil-common.xsd"/>
  <xsd:import namespace="http://data.europa.eu/edm/1/ns/toe"
              schemaLocation="./toe-common.xsd"/>
  <!-- ===================================================== -->

  <xsd:element name="Toe_FormB" type="Toe_FormB_Type"/>

  <xsd:complexType name="Toe_FormB_Type">
    <xsd:sequence>
      <xsd:element name="Section1To3" type="toe:Toe_ReferenceIssuer_Type"/>
      <xsd:element name="Section4" type="civil:Civil_TransmissionEntity_Type"/>
      <xsd:element name="Section5" type="udt:DateType" minOccurs="0"/>
      <xsd:element name="Section6" type="Toe_FormB_RequestCannotBeDealWith_Type"/>
      <xsd:element name="Section7" type="civil:Civil_DoneAtDate_Type"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormB_RequestCannotBeDealWith_Type">
    <xsd:sequence>
      <xsd:element name="LanguageNotAccepted" type="Toe_FormB_LanguageNotAcceptedContainer_Type" minOccurs="0"/>
      <xsd:element name="DocumentNotLegible" type="udt:IndicatorType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormB_LanguageNotAcceptedContainer_Type">
    <xsd:sequence>
      <xsd:element name="AcceptedLanguage" type="civil:Civil_EuLanguage_Type" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

</xsd:schema>
