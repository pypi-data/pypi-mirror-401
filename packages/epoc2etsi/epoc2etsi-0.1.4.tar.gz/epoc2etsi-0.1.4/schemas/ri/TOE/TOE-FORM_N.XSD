<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
  targetNamespace="http://data.europa.eu/edm/1/ns/forms/TOE-FORM_N.XSD#"
  xmlns="http://data.europa.eu/edm/1/ns/forms/TOE-FORM_N.XSD#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:udt="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
  xmlns:civil="http://data.europa.eu/edm/1/ns/civil"
  xmlns:toe="http://data.europa.eu/edm/1/ns/toe"
  elementFormDefault="qualified"
  version="1.0">

  <!-- ===== Imports======================================== -->
  <xsd:import namespace="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
              schemaLocation="../_external/UBL-2.1/xsdrt/common/UBL-UnqualifiedDataTypes-2.1.xsd"/>
  <xsd:import namespace="http://data.europa.eu/edm/1/ns/civil" schemaLocation="../_common/civil-common.xsd"/>
  <xsd:import namespace="http://data.europa.eu/edm/1/ns/toe" schemaLocation="./toe-common.xsd"/>
  <!-- ===================================================== -->

  <xsd:element name="Toe_FormN" type="Toe_FormN_Type"/>

  <xsd:complexType name="Toe_FormN_Type">
    <xsd:sequence>
      <xsd:element name="Section1To4" type="toe:Toe_ReferenceIssuerExecutor_Type"/>
      <xsd:element name="Section5To6" type="Toe_FormN_TechnicalDataOfRequestingCourt_Type"/>
      <xsd:element name="Section7To8" type="Toe_FormN_PreferredDatesAndTimesOfConnections_Type"/>
      <xsd:element name="Section9To11" type="Toe_FormN_InformationOnInterpretationAndRecording_Type"/>
      <xsd:element name="Section12" type="civil:Civil_DoneAtDate_Type"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormN_InformationOnInterpretationAndRecording_Type">
    <xsd:sequence>
      <xsd:element name="AssistanceForFindingAnInterpreterRequested" type="udt:IndicatorType" minOccurs="0"/>
      <xsd:element name="RelevantLanguages" type="civil:Civil_EuLanguages_Type"/>
      <xsd:element name="OtherLanguage" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="RecordingOfTakingOfEvidenceWillBeMade" type="udt:IndicatorType" minOccurs="0"/>
      <xsd:element name="Other" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormN_TechnicalDataOfRequestingCourt_Type">
    <xsd:sequence>
      <xsd:element name="Isdn" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="Ip" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="TelCourtRoom" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="Other" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="PreferredFormOfConnection" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormN_PreferredDatesAndTimesOfConnections_Type">
    <xsd:sequence>
      <xsd:element name="Connections" type="Toe_FormN_DatesTimes_Type"/>
      <xsd:element name="TestConnections" type="Toe_FormN_DatesTimes_Type"/>
      <xsd:element name="TestConnectionContactPerson" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="TestConnectionLanguages" type="civil:Civil_EuLanguages_Type"/>
      <xsd:element name="TestConnectionLanguageOther" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="TestConnectionDifficultiesTelephone" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormN_DatesTimes_Type">
    <xsd:sequence>
      <xsd:element name="DateTime" type="udt:DateTimeType" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>

</xsd:schema>
