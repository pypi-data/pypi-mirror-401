<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
  targetNamespace="http://data.europa.eu/edm/1/ns/forms/TOE-FORM_D_REPLY.XSD#"
  xmlns="http://data.europa.eu/edm/1/ns/forms/TOE-FORM_D_REPLY.XSD#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:udt="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
  xmlns:civil="http://data.europa.eu/edm/1/ns/civil"
  elementFormDefault="qualified"
  version="1.0">

  <!-- ===== Imports======================================== -->
  <xsd:import namespace="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
              schemaLocation="../_external/UBL-2.1/xsdrt/common/UBL-UnqualifiedDataTypes-2.1.xsd"/>
  <xsd:import namespace="http://data.europa.eu/edm/1/ns/civil" schemaLocation="../_common/civil-common.xsd"/>
  <!-- ===================================================== -->

  <xsd:element name="Toe_FormD_Reply" type="Toe_FormDReply_Type"/>

  <xsd:complexType name="Toe_FormDReply_Type">
    <xsd:sequence>
      <xsd:element name="Section1To2" type="civil:Civil_ReferenceNumbers_Type"/>
      <xsd:element name="Section3To4" type="Toe_FormDReply_AdditionalInformationDepositOrAdvanceRequired_Type"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormDReply_AdditionalInformationDepositOrAdvanceRequired_Type">
    <xsd:sequence>
      <xsd:element name="AdditionalInformation" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="DepositOrAdvance" type="Toe_FormDReply_DepositOrAdvance_Container_Type" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormDReply_DepositOrAdvance_Container_Type">
    <xsd:sequence>
      <xsd:element name="Value" type="Toe_FormDReply_DepositOrAdvance_Type" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormDReply_DepositOrAdvance_Type">
    <xsd:sequence>
      <xsd:element name="PaymentConfirmation" type="Toe_FormDReply_PaymentConfirmation_Container_Type" minOccurs="0"/>
      <xsd:element name="RequestedInformation" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormDReply_PaymentConfirmation_Container_Type">
    <xsd:sequence>
      <xsd:element name="Value" type="Toe_FormDReply_PaymentConfirmation_Type" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormDReply_PaymentConfirmation_Type">
    <xsd:sequence>
      <xsd:element name="ProofOfPaymentAttached" type="udt:IndicatorType" minOccurs="0"/>
      <xsd:element name="DateOfPayment" type="udt:DateType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

</xsd:schema>
