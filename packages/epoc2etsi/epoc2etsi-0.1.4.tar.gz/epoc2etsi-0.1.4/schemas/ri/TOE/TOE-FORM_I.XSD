<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
  targetNamespace="http://data.europa.eu/edm/1/ns/forms/TOE-FORM_I.XSD#"
  xmlns="http://data.europa.eu/edm/1/ns/forms/TOE-FORM_I.XSD#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:udt="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
  xmlns:civil="http://data.europa.eu/edm/1/ns/civil"
  elementFormDefault="qualified"
  version="1.0">

  <!-- ===== Imports======================================== -->
  <xsd:import namespace="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
              schemaLocation="../_external/UBL-2.1/xsdrt/common/UBL-UnqualifiedDataTypes-2.1.xsd"/>
  <xsd:import namespace="http://data.europa.eu/edm/1/ns/civil"
              schemaLocation="../_common/civil-common.xsd"/>
  <!-- ===================================================== -->

  <xsd:element name="Toe_FormI" type="Toe_FormI_Type"/>

  <xsd:complexType name="Toe_FormI_Type">
    <xsd:sequence>
      <xsd:element name="Section1To2" type="civil:Civil_ReferenceNumbers_Type"/>
      <xsd:element name="Section3" type="civil:Civil_TransmissionEntity_Type"/>
      <xsd:element name="Section4" type="civil:Civil_TransmissionEntity_Type"/>
      <xsd:element name="Section5To6" type="Toe_FormI_DateTimePlace_Type"/>
      <xsd:element name="Section7To8" type="Toe_FormI_ParticipationConditions_Type"/>
      <xsd:element name="Section9" type="civil:Civil_DoneAtDate_Type"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormI_DateTimePlace_Type">
    <xsd:sequence>
      <xsd:element name="DateTime" type="udt:DateTimeType" minOccurs="0"/>
      <xsd:element name="Place" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Toe_FormI_ParticipationConditions_Type">
    <xsd:sequence>
      <xsd:element name="PartiesAndRepresentatives" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="requestingCourtRepresentatives" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
