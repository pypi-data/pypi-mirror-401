<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
  targetNamespace="http://data.europa.eu/edm/1/ns/forms/SOD-FORM_B.XSD#"
  xmlns="http://data.europa.eu/edm/1/ns/forms/SOD-FORM_B.XSD#"
  xmlns:udt="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:civil="http://data.europa.eu/edm/1/ns/civil"
  elementFormDefault="qualified"
  attributeFormDefault="unqualified"
  version="1.0">

  <!-- ===== Imports======================================== -->
  <xsd:import namespace="http://www.w3.org/ns/corevocabulary/AggregateComponents"
              schemaLocation="../_external/Core_Vocabularies_XML_Schema_v1.1/xsd/CoreVocabularies-AggregateComponents-1.1.xsd"/>
  <xsd:import namespace="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
              schemaLocation="../_external/UBL-2.1/xsdrt/common/UBL-UnqualifiedDataTypes-2.1.xsd"/>
  <xsd:import namespace="http://data.ecodex.eu/codelist-EULanguageCode"
              schemaLocation="../_external/MLACase_BD009001_2/codelist/standard/Europeanunion_EULanguage_.xsd"/>
  <xsd:import namespace="http://data.europa.eu/edm/1/ns/sod"
              schemaLocation="./sod-common.xsd"/>
  <xsd:import namespace="http://data.europa.eu/edm/1/ns/civil"
              schemaLocation="../_common/civil-common.xsd"/>
  <!-- ===================================================== -->

  <xsd:element name="Sod_FormB" type="Sod_FormB_Type"/>

  <xsd:complexType name="Sod_FormB_Type">
    <xsd:sequence>
      <xsd:element name="Section0" type="udt:TextType"/>
      <xsd:element name="Section1" type="civil:Civil_TransmissionEntity_Type"/>
      <xsd:element name="Section2" type="civil:Civil_TransmissionEntity_Type" minOccurs="0"/>
      <xsd:element name="Section3" type="Sod_FormB_AddresseeOfAddressToDetermine_Type"/>
      <xsd:element name="Section4" type="civil:Civil_DoneAtDate_Type"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Sod_FormB_AddresseeOfAddressToDetermine_Type">
    <xsd:sequence>
      <xsd:element name="EntityInfo" type="civil:Civil_EntityInfo_Type"/>
      <xsd:element name="KnownDetailsOfLegalPerson" type="Sod_FormB_KnownDetailsOfLegalPerson_Type"/>
      <xsd:element name="KnownDetailsOfNaturalPerson" type="Sod_FormB_KnownDetailsOfNaturalPerson_Type"/>
      <xsd:element name="OtherInformation" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Sod_FormB_KnownDetailsOfLegalPerson_Type">
    <xsd:sequence>
      <xsd:element name="RegistrationNumberOrEquivalent" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="BoardMembersNames" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Sod_FormB_KnownDetailsOfNaturalPerson_Type">
    <xsd:sequence>
      <xsd:element name="NameAtBirth" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="OtherKnownNames" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="DateAndPlaceOfBirth" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="IdNumberOrEquivalent" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="MothersOrFathersNameAtBirth" type="udt:TextType" minOccurs="0"/>
      <xsd:element name="OtherInformation" type="udt:TextType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>

</xsd:schema>
