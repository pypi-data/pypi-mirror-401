<xsd:schema
  targetNamespace="http://data.europa.eu/edm/1/ns/forms/EIO-A#"
  xmlns="http://data.europa.eu/edm/1/ns/forms/EIO-A#"
  xmlns:eio="http://data.europa.eu/edm/1/ns/eio"
  xmlns:cva="http://www.w3.org/ns/corevocabulary/AggregateComponents"
  xmlns:udt="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  elementFormDefault="qualified"
  attributeFormDefault="unqualified"
  version="1.0">

