          <xsd:documentation xml:lang="en">
            corruption
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Fraud" type="udt:IndicatorType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            fraud, including that affecting the financial interests of the European Union within the meaning of the
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ConventionOnTheProtectionOfFinancialInterests" type="udt:IndicatorType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Convention of 26 July 1995 on the protection of the European Communities� financial interests
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LaunderingOfTheProceedsOfCrime" type="udt:IndicatorType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            laundering of the proceeds of crime
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CounterfeitingCurrency" type="udt:IndicatorType" minOccurs="0">
