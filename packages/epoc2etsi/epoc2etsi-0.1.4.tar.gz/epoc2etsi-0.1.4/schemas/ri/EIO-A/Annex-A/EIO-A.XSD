          <xsd:documentation xml:lang="en">
            illicit trafficking in weapons, munitions and explosives
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Corruption" type="udt:IndicatorType" minOccurs="0">
