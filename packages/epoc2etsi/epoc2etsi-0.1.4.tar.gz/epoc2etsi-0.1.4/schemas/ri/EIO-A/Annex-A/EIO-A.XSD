          <xsd:documentation xml:lang="en">
            time and duration of connections
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CallAttempts" type="udt:IndicatorType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            call attempts
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
