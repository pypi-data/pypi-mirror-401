        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            base station ID, including geographical information (X/Y coordinates), at the time of initiation and
            termination of the connection
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BearerTeleserviceUsed" type="udt:IndicatorType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            bearer / teleservice used (e.g. UMTS, GPRS)
