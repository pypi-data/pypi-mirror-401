% Engine-agnostic thickness control
\newcommand{\thickText}[2][0.9]{%
  \ifpdftex\pdfliteral{2 Tr #1 w}#2\pdfliteral{0 Tr}%
  \else\ifluatex\pdfextension literal direct {2 Tr #1 w}#2\pdfextension literal direct {0 Tr}%
  \else\ifxetex\special{pdf:literal direct 2 Tr #1 w}#2\special{pdf:literal direct 0 Tr}\fi\fi\fi
}

\newcommand{\omrCirc}[2][white]{%
\hspace{-0.1em}%
\begin{tikzpicture}[
    baseline={([yshift=-0.7ex] current bounding box.center)},
  ]   
  \node[
    draw, 
    circle, 
    fill=#1,
    scale=0.58,
    line width=1pt,
    inner sep=2pt, 
    minimum size=1em,
    text width=1em, 
    align=center,
    font=\sffamily\small
  ] {\thickText[0.8]{#2}};
\end{tikzpicture}%
\hspace{-0.1em}%
\xspace%
}

\foreach \x in {A,...,Z} {
  \expandafter\xdef\csname o\x\endcsname{\noexpand\omrCirc{\x}}
  \expandafter\xdef\csname x\x\endcsname{\noexpand\omrCirc[black!60]{\x}}
}

