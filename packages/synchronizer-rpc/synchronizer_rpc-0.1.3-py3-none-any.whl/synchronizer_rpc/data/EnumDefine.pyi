from enum import IntEnum
from synchronizer_rpc.proto.syncProc_pb2 import ErrorData

__all__ = ['EStatus', 'EError', 'EErrorDesp']

class EStatus(IntEnum):
    DISCONNECTED = ...
    IDLE = ...
    INIT = ...
    START = ...
    PPS_ON = ...
    STOP = ...
    DONE = ...
    ERROR = ...
    BLOCKED = ...
    @staticmethod
    def all(): ...

class EErrorDesp(IntEnum):
    NO_ERROR = ...
    NOT_CONNECTED = ...
    BUSY = ...
    FAIL_KEEP_ALIVE = ...
    FAIL_REG_SERVICE = ...
    UNINTENDED_ERROR = ...
    @staticmethod
    def all(): ...

class EError:
    errno: EErrorDesp
    msg: str
    def __init__(self, err: ErrorData) -> None: ...
