{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OpenProject Project",
  "type": "object",
  "description": "Schema for OpenProject projects - automatically generated by Singer SDK",
  "properties": {
    "id": {
      "type": "integer",
      "description": "Unique project identifier"
    },
    "_type": {
      "type": ["string", "null"],
      "description": "Resource type"
    },
    "identifier": {
      "type": ["string", "null"],
      "description": "Project key/identifier"
    },
    "name": {
      "type": ["string", "null"],
      "description": "Project name"
    },
    "active": {
      "type": ["boolean", "null"],
      "description": "Whether project is active"
    },
    "public": {
      "type": ["boolean", "null"],
      "description": "Whether project is public"
    },
    "description": {
      "type": ["object", "null"],
      "description": "Project description with formatting",
      "properties": {
        "format": { "type": ["string", "null"] },
        "raw": { "type": ["string", "null"] },
        "html": { "type": ["string", "null"] }
      }
    },
    "status": {
      "type": ["string", "null"],
      "description": "Project status"
    },
    "statusExplanation": {
      "type": ["object", "null"],
      "description": "Status explanation",
      "properties": {
        "format": { "type": ["string", "null"] },
        "raw": { "type": ["string", "null"] },
        "html": { "type": ["string", "null"] }
      }
    },
    "createdAt": {
      "type": ["string", "null"],
      "format": "date-time",
      "description": "Creation timestamp"
    },
    "updatedAt": {
      "type": ["string", "null"],
      "format": "date-time",
      "description": "Last update timestamp - used for incremental sync"
    },
    "_links": {
      "type": ["object", "null"],
      "description": "HAL hypermedia links"
    }
  },
  "required": ["id"]
}
